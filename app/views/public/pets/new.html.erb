<h1>ペット登録</h1>

<%= form_with  model: Pet.new do |f| %>

  <h4>写真</h4>
  <%= f.file_field :image, accept: "image/*" %>

  <h4>名前</h4>
  <%= f.text_field :name %>
	
  <h4>種類</h4>
  <%= f.text_field :type %>
  
  <h4>誕生日</h4>
  <%= f.text_field :birthday %>

  <h4>性別</h4>
  <%= f.text_field :bsex %>

  <h4>紹介文</h4>
  <%= f.text_area :introduction %>
  
  <%= f.submit '登録' %>
<% end %>