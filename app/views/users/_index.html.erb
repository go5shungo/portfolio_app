<table class='table'>
  <thead>
    <tr>
      <th>画像</th>
      <th>名前</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td><%= image_tag user.profile_image, size: '50x50' %></td>
        <td><%= user.name %></td>
        <td><%= link_to '詳細', user, class: "user_#{user.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
