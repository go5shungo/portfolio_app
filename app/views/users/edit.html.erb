<h2>プロフィール編集</h2>
<%= form_with model: @user do |f| %>
  <p><label for="inputName">名前</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%></p>
  <p><label for="inputImage">写真</label>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %></p>
  <%= f.submit "変更を保存" %>
<% end %>